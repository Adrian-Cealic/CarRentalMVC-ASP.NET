{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "IIS Express",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "C:\\Program Files\\IIS Express\\iisexpress.exe",
            "args": [
                "/path:${workspaceFolder}\\CarRentalMVC+ASP.NET",
                "/port:62269"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "listening on port ([0-9]+)",
                "uriFormat": "http://localhost:%s"
            }
        }
    ]
} 